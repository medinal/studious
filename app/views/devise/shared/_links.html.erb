<%- if controller_name != 'sessions' %>
  <%= link_to "Already a user? Log in", new_session_path(resource_name), class: "text-center float-center" %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Not registered? Sign up here!", new_registration_path(resource_name), class: "text-center float-center" %><br />
<% end -%>
