<div id="main" class="small-12 medium-10 columns">
  <h1>Portfolios</h1>

  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @portfolios.each do |portfolio| %>
      <tr>
        <td><%= link_to 'Show', student_portfolio_path(portfolio) %></td>
        <td><%= link_to 'Edit', edit_student_portfolio_path(portfolio) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Portfolio', new_student_portfolio_path %>
</div
